  "author": "ɛʟʟɨօȶ b  =^._.^= ∫ <elliot@email.gd>",
  "license": "The Unlicense",
  "main": "index.js",
  "bin": {
    "kvuploader": "./index.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "dependencies": {
    "chalk": "^5.3.0",
    "fdir": "^6.4.2",
    "mime-types": "^2.1.35",
    "minimatch": "^10.0.1",
    "toml": "^3.0.0"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin-js": "^2.10.0",
    "elliotisms": "^0.0.49",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.12.0",
    "eslint-plugin-perfectionist": "^3.9.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^2.0.4",
    "eslint-plugin-unicorn": "^56.0.0",
    "prettier": "^3.3.3",
    "release-it": "^17.10.0"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSameLine": true,
    "bracketSpacing": true,
    "semi": false,
    "experimentalTernaries": true,
    "singleQuote": false,
    "jsxSingleQuote": false,
    "quoteProps": "as-needed",
    "trailingComma": "none",
    "singleAttributePerLine": false,
    "htmlWhitespaceSensitivity": "css",
    "vueIndentScriptAndStyle": false,
    "proseWrap": "preserve",
    "insertPragma": false,
    "printWidth": 80,
    "requirePragma": false,
    "tabWidth": 2,
    "useTabs": false,
    "embeddedLanguageFormatting": "auto"
  },
  "eslintConfig": {
    "root": true,
    "plugins": [
      "prettier",
      "@stylistic/js",
      "unicorn",
      "simple-import-sort"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended",
      "plugin:unicorn/recommended"
    ],
    "env": {
      "node": true,
      "es6": true,
      "es2024": true
    },
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "off"
    }
  }
}
